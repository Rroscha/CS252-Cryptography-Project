[flake8]
ignore =
    ;W503 line break before binary operator
    W503,
    ;E203 whitespace before ':'
    E203,
    E221,
    E114,
    E116,
    E266,
    F401,
    W605,
    W293,

; exclude file
exclude =
    .tox,
    .git,
    __pycache__,
    build,
    dist,
    *.pyc,
    *.egg-info,
    .cache,
    .eggs
    .venv
    venv

max-line-length = 120